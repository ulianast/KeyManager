{
  "name": "key-manager",
  "version": "0.0.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "scripts": {
    "build": "webpack --mode production",
    "build-dev": "webpack --mode development",
    "start": "set NODE_ENV=development&& nodemon --exec npm-run-all --parallel start-app lint:watch",
    "seed": "sequelize db:seed:all",
    "security-check": "nsp check",
    "start-app": "babel-node --inspect ./bin/www",
    "lint": "eslint --fix app.js bin client/src server config --color",
    "lint:watch": "esw -w --fix --color app.js bin client/src server config"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "express": "~4.16.0",
    "express-force-ssl": "^0.3.2",
    "express-session": "^1.15.6",
    "helmet": "^3.11.0",
    "jsonwebtoken": "^8.2.1",
    "moment": "^2.22.1",
    "multiparty": "^4.1.4",
    "nconf": "0.10",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.8.3",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "randomstring": "^1.1.5",
    "sequelize": "^4.34",
    "sequelize-cli": "^4.0.0",
    "winston": "2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.18.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
    "file-loader": "^1.1.11",
    "nodemon": "^1.15.1",
    "npm-run-all": "^4.1.2",
    "style-loader": "^0.20.3",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.0"
  }
}
